<strong>My Blog Page</strong>
<br>
<a href="/new">Create New Blog Input</a>
<ul>
	<% @blogs.each do |blog| %>
	<li>
		<strong>Title:
		<a href="/blog/<%= blog.id %>"><%= blog.title %></a></strong><br>
		<strong>Day created:</strong>
		<%= blog.date %><br>
		<strong>Blog Entry:</strong><br>
		<%= blog.writeup %><br>
		<a href="/update_blog/<%= blog.id %>">edit this entry</a></strong></li>
		<br>
	<% end %>
</ul>
